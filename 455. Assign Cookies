
class Solution {
    public int findContentChildren(int[] g, int[] s) {
       if(s.length==0) return 0;
       Arrays.sort(g);
       Arrays.sort(s);
       
       int ans=0;
       int sl=s.length-1;
       int gl=g.length-1;
       while(sl>=0 && gl>=0){
           if(s[sl]>=g[gl]){
               ans++;
               gl--;
               sl--;
           }else{
               gl--;
           }
       }
       return ans;
    }
}