class Solution {
    public int maxFrequencyElements(int[] nums) {
        HashMap<Integer,Integer> freq=new HashMap<>();
        for(int el:nums){
            if(!freq.containsKey(el)){
                freq.put(el,1);
            }else{
                freq.put(el,freq.get(el)+1);
            }
        }
        int n=Integer.MIN_VALUE;
       for(Integer key:freq.keySet()){
           if(n<freq.get(key)){
               n=freq.get(key);
           }
       }
       int ans=0;
       System.out.print(n);
        for(Integer key:freq.keySet()){
           if(n==freq.get(key)){
               ans+=freq.get(key);
           }
       }
       return ans;
    }
}